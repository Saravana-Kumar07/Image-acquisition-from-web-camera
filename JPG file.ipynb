   "source": [
    "import cv2\n",
    "v=cv2.VideoCapture(0)\n",
    "while True:\n",
    "    ret,frame1=v.read()\n",
    "    cv2.imwrite('IMG.jpg',frame1)\n",
    "    if cv2.waitKey(1)==ord('q'):\n",
    "        break\n",
    "v.release()\n",
    "cv2.destroyAllWindows()   "
   ]
